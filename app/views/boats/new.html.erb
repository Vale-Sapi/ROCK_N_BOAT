<h1 class="serious-font">Boats</h1>
<p class="serious-font">Please fill out the form</p>
<%= simple_form_for @boat do |f| %>
  <%= f.input :name %>
  <%= f.input :location %>
  <%= f.input :details %>
  <%= f.input :price %>
  <%= f.input :photo, as: :file %>
  <%= f.submit class: 'btn btn-flat' %>
<% end %>
